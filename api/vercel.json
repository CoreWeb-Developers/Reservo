{
  "version": 2,
  "builds": [
    {
      "src": "./index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/server/$1" },
    { "src": "/admin/(.*)", "dest": "admin/$1" },
    { "src": "/(.*)", "dest": "client/$1" }
  ],
  "env": {
    "CLIENT_URL": "https://reservo.netlify.app/",
    "ADMIN_URL": "https://reservo-admin.netlify.app/",
    "SERVER_URL": "https://reservo-api.vercel.app/",
    "SERVER_PORT": "80"
  }
}
